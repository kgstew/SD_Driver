{
  "configurations": [
    {
      "name": "STM32F4",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/Inc",
        "${workspaceFolder}/Drivers/Device/STM32F4xx/Include",
        "${workspaceFolder}/Drivers/CMSIS/Include"
      ],
      "defines": ["USE_FULL_LL_DRIVER", "STM32F446RE"],
      "macFrameworkPath": [
        "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c11",
      "intelliSenseMode": "macos-gcc-arm",
      "cppStandard": "c++20"
    }
  ],
  "version": 4
}
